	        printCount(numList);
	    }
	    public static void printCount(ArrayList<Integer> list) {
	        int[] count = new int[10];
	        // 对应保存数字出现的次数
	        for (int i = 0; i < list.size(); i++) {
	            int c = list.get(i);
