```java
	public class Test2 {
	    public static void main(String[] args) {
	        Scanner scanner = new Scanner(System.in);
	        ArrayList<Student> list = new ArrayList<>();
	        while (true) {
	            System.out.println("1.录入信息 0.退出");
	            int i = scanner.nextInt();
	            switch (i) {
	                case 1:
	                    inputStu(list , scanner);
	                    break;
