	    }
	    public static ArrayList<Integer> getNumList() {
	        ArrayList<Integer> list = new ArrayList<>();
	        Random r = new Random();
	        for (int i = 0; i < 100; i++) {
	            int x = r.nextInt(10) + 1;
	            list.add(x);
	        }
	        return list;
	    }
	}
